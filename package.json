{
  "name": "mov-interact-website",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron-starter.js",
  "proxy": "http://localhost:8000/",
  "homepage": "./",
  "dependencies": {
    "axios": "^0.25.0",
    "evergreen-ui": "^6.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "nf start",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/logo.ico --out=release-builds",
    "package-win": "electron-packager . reactron --overwrite --platform=win32 --arch=x64 --icon=src/assets/icons/logo.ico --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . reactron --overwrite --platform=linux --arch=x64 --icon=src/assets/images/logo.png --out=release-builds"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-packager": "^15.4.0",
    "foreman": "^3.0.1",
    "sass": "^1.49.7"
  }
}
